<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>USUARIOS</title>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.css">
	<link rel="stylesheet" href="css/styles.css">

</head>
<body>
	
	<!-- Navbar goes here -->
	<div class="container">
		<h1>Bienvenidos!</h1>
	</div>
    <!-- Page Layout here -->
	<div class="row">
		
		<div class="col s4">
			<!-- Grey navigation panel -->
			<div class="container">
				<h1>Bienvenidos!</h1>
		        <ul class="collection">
					<% registros.forEach(function(item,ind){ %>
					    <li class="collection-item avatar">
							<img src="http://lorempixel.com/100/100/people" alt="" class="circle">
							<span class="title">Title</span>
							<p><%= item.nombre %> <br>
							 <%= item.apellido %>
							</p>
							<a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
					    </li>
					<% }) %>
				</ul>
		  	</div>
		</div>

		<div class="col s8">
			<!-- Teal page content  -->
			
      </div>

    </div>


	
	


</body>
</html>